/// Interface HTTP Gateway
///
/// ## Permissions
///
/// This API is ALWAYS available.


/// Logging API Interface
interface event {

    use hermes:binary/api.{bstr};

    // headers kv
    type kv-headers = list<tuple<string, string>>;

    // http response code 
    type code = u16;

    // body
    type body = bstr;

    // response
    type response = tuple<code, kv-headers, body>;

    /// Reply
    reply: func(body: bstr, headers: bstr, path: string, method: string) -> response;
    
}