// cspell: words

/// CBOR API Interface - Imports ONLY
interface api {
    /// CBOR is a binary cbor data type.
    ///    This type is used to indicate the binary array MUST be CBOR data.
    type cbor = list<u8>;
}


/// World just for the Hermes 'cron' API and Event.
world cbor-api {
    import api;
}
  