// cspell: words

/// Binary API Interface - Imports ONLY
interface api {
    /// Binary String (bstr) is a list of bytes.
    ///    This type is used to indicate the data is an arbitrary array of bytes.
    type bstr = list<u8>;
}


/// World just for the Hermes 'cron' API and Event.
world binary-api {
    import api;
}
  